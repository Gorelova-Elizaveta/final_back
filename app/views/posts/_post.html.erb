<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Text:</strong>
    <%= post.text %>
  </p>

  <p>
    <strong>Tag:</strong>
    <%= post.tag %>
  </p>

  <p>
    <strong>Userid:</strong>
    <%= post.userID %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= post.image %>
  </p>

</div>
